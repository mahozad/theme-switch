{
  "minified": true,
  "comments": false,
  "sourceMaps": "both",
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": "auto",
        "targets": {
          "node": true,
          "edge": "80",
          "firefox": "80",
          "chrome": "80",
          "safari": "13"
        }
      }
    ]
  ],
  "plugins": [
    [
      "minify-mangle-names",
      {
        "topLevel": false,
        "keepFnName": false,
        "keepClassName": false
      }
    ],
    "rewire-test-env-only",
    "iife-wrap"
  ]
}
